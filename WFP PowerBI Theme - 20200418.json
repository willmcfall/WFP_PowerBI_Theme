{
	"name": "MapTemplate",
	"visualStyles": {
		"map": {
			"*": {
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": true,
					"titleText": "",
					"legendColor": { "solid": { "color": "#01B8AA"}},
					"fontFamily": "Segoe UI",
					"fontSize": 12
				}],
				"dataPoint": [{
					"defaultCategoryColor": { "solid": { "color": "#01B1AA"}},
